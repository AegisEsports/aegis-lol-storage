    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "kysely": "0.23.3",
    "morgan": "^1.10.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chai": "^4.3.20",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.10",
    "@types/node": "^16.18.11",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "axios": "^1.11.0",
    "chai": "^4.5.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-security-node": "^1.1.4",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
