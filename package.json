{
  "name": "aegis_lol_storage",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:win": "del dist && del .tsbuildinfo && tsc",
    "dev": "sh -c npx tsx watch src/migrate.ts && npm run start",
    "start": "npx tsx watch src/server.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --remove-orphans",
    "docker:clean": "docker compose down --rmi local --remove-orphans",
    "docker:remove": "docker compose down -v --remove-orphans",
    "test": "mocha --timeout 5000 -r ts-node/register test/**/*.test.ts",
    "migrate:latest": "node dist/migrate-to-latest.js"
  },
  "author": "[Aegis] Doowan",
  "license": "GPL-3.0",
