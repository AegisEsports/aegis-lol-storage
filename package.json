{
  "name": "aegis_lol_storage",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "migrate": "node dist/migrate-to-latest.js",
    "build": "rm -rf dist && tsc",
    "build:win": "del dist && del .tsbuildinfo && tsc",
    "build:swc": "swc src -d dist --strip-leading-paths --copy-files",
    "postbuild:swc": "tsc-alias -p tsconfig.json --outDir dist",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --remove-orphans",
    "docker:clean": "docker compose down --rmi local --remove-orphans",
    "docker:wipe": "docker compose down -v --remove-orphans",
    "docker:reset": "npm run docker:clean && npm run docker:wipe && npm run docker:up && npm run db-init",
    "db-init": "tsx src/migrate.ts",
    "db-access": "docker exec -it postgres psql -U postgres -d aegis_lol_storage",
    "test": "mocha --timeout 5000 -r ts-node/register test/**/*.test.ts",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "author": "[Aegis] Doowan",
  "license": "GPL-3.0",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "kysely": "^0.28.5",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-error-enum": "^1.0.2",
    "twisted": "^1.72.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@types/chai": "^4.3.20",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.10",
    "@types/node": "^16.18.11",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "axios": "^1.12.1",
    "chai": "^4.5.0",
    "cookie-parser": "^1.4.7",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-security-node": "^1.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0"
  }
}
